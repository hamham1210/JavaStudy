<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MemberVOmapper">
	<insert id="memberAdd" parameterType="memberVO">
		INSERT
		INTO member(id,password,name,address)
		values(#{id},#{password},#{name},#{address})
	</insert>
	<select id="memberList" resultType="memberVO">
	SELECT
	id, name, address
	from member
	</select>
	<select id="getMember" parameterType="string" resultType="user">
	select
	id,password,name,address
	from member
	where id like concat('%',#{value})
	</select>
	<delete id="deleteMember" parameterType="string">
	DELETE from member where user_id=#{value}
	
	</delete>
</mapper>