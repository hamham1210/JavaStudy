package com.service.mybatis.unit;

import java.io.IOException;

import org.apache.ibatis.session.SqlSession;
import org.apache.ibatis.session.SqlSessionFactory;

import com.mysql.cj.Session;

import spring.service.domain.MemberVO;

public class MyBatisMemberTestApp {
	
	public static void main(String[] args) throws IOException {
	SqlSessionFactory factory =FactoryService.getFactory();
		MemberVO pvo = new MemberVO("kosta","1234");
	//3)SqlSession 생성
			SqlSession session =factory.openSession();
			session.insert("MemberVOmapper.memberAdd",pvo);
			
			System.out.println();
			session.selectList("MemberVOmapper.memberList")
			.forEach(c->System.out.println(c));
			
			session.commit();
			session.close();
}
}
